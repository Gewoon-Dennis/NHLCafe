@page
@model NHLCafe.Pages.AccountOverview

@{
    ViewData["Title"] = "Account Overview";
}
<div>
    <div class="container border" style="padding: 20px">
        <div class="row">
            <div class="col-2">
                <label>User-id: </label>
            </div>
            <div class="col-4">
                <label>@Model.OberId</label>
            </div>
        </div>
        <div class="row">
            <div class="col-2">
                <label>Username: </label>
            </div>
            <div class="col-4">
                <label>@Model.OberName</label>
            </div>
        </div>
        <div class="row">
            <div class="col-2">
                <label>Workplace: </label>
            </div>
            <div class="col-4">
                <label>@Model.OberWorkplace</label>
            </div>
        </div>
    </div>
</div>
